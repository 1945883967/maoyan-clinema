<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.rest.common.persistence.dao.FilmActorTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.rest.common.persistence.model.FilmActorT">
        <id column="UUID" property="uuid" />
        <result column="film_id" property="filmId" />
        <result column="actor_id" property="actorId" />
        <result column="role_name" property="roleName" />
    </resultMap>


    <insert id="insertFilmActors" useGeneratedKeys="true"
            keyProperty="id">
        INSERT INTO film_actor_t (film_id,actor_id,role_name) VALUES
        <foreach item="item" collection="list" separator=",">
            (#{item.filmId}, #{item.actorId}, #{item.roleName})
        </foreach>
    </insert>
</mapper>
